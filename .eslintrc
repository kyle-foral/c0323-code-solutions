{
  "root": true,
  "env": {
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2023
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "eslint:recommended",
    "standard"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {

    "no-console": "off",
    "object-shorthand": "off"
  },
  "overrides":[
    {
      "files": [
        "**/*.html"
      ],
      "plugins": [
        "eslint-plugin-html"
      ]
    }
  ]
}
